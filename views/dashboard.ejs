<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
</head>
<body>

  <%- include('../components/navbar.ejs') %>

      
        

        <div class="container">
          <div class="row">

          <% Array.from(link).forEach(function(row) { %>
            <div class="col-md-3">
              <div class="card mt-4">
                <div class="card-header">
                 Link Name :   : <b><%= row.linkname %> <a href="/link_update/<%= row.id %>">Update</a></b> 
                </div>
                <div class="card-body">
                  <p class="card-text"> Description : <%= row.description %> </p>
                  <p class="card-text" id="#<%= row.file_grup %>" > File Grup : <%= row.file_grup %></p>
                  <a href=" <%= row.link %>" target="_blank" class="btn btn-primary">Go To Link</a>
                  <a href="/link_delete?id=<%= row.id %>" class=" btn btn-sm btn-danger"> Delete </a>
                </div>
              </div>
            </div>
          <% }); %>


          
            
            </div>
            
          </div>





      
           
      
        </div>
        

      

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"  crossorigin="anonymous"></script> 
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js"></script>

<script>

</script>
</body>
</html>